<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gaossalma.guide_login">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".listview.ListBeritaItemActivity" />
        <activity android:name=".DetailListBeritaActivity" />
        <activity android:name=".ListDestinasiActivity" />
        <activity android:name=".listview.ListDestinasiItemActivity" />
        <activity android:name=".listview.ListPilihDestinasiItemActivity" />
        <activity android:name=".DetailListDestinasiActivity" />
        <activity android:name=".PilihDestinasiActivity" />
        <activity android:name=".PilihDestinasiItemActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".pemandu.LoginPemanduActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pemandu.PemanduMenuActivity" />
        <activity android:name=".pemandu.PemanduProfilActivity" />
        <activity android:name=".HasilActivity"></activity>
    </application>

</manifest>